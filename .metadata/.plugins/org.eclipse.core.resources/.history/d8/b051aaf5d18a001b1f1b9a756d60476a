package self;

import java.util.HashSet;

public class BaseballGame_Set {
	public static void main(String[] args) {
	
		HashSet<Integer> random = new HashSet<Integer>();
		
		while(random.size() < 3) {
			int num = (int)(Math.random() * 9 + 1);
			random.add(num);
		}
		
		System.out.println(random);
		
		HashSet<Integer> insert = new HashSet<Integer>();
		
		int count = 0;
		while(true) {
		int firstInput = 0;
		int secongInput = 0;
		int thirdInput = 0;
		
		insert.add(firstInput);
		insert.add(secongInput);
		insert.add(thirdInput);
		
		insert = new HashSet<>();
		}
		
		
	}
}
